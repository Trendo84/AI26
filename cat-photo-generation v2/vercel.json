{
  "version": 2,
  "builds": [
    {
      "src": "api/generate.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/prediction.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/generate",
      "dest": "api/generate.js"
    },
    {
      "src": "/api/prediction",
      "dest": "api/prediction.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
